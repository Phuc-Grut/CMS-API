{
  "ConnectionStrings": {
    "DAMConnection": "Server={data_zone};Database={data};User ID={APP_SQL_USERNAME};Password={APP_SQL_PASSWORD};MultipleActiveResultSets=true",
    "DAMEventConnection": "Server={data_zone};Database={data};User ID={APP_SQL_USERNAME};Password={APP_SQL_PASSWORD};MultipleActiveResultSets=true",
    "Redis": "{APP_REDIS_CONNECTION}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AppConfig": {
    "UrlPrefix": "{APP_ROUTE_PREFIX}"
  },
  "FileUpload": {
    "RootFolder": "data",
    "ImageAllowExtension": "png;jpeg;jpg",
    "FileAllowExtension": "txt;rar;zip;xls;xlsx;doc;docx;png;jpeg;jpg",

    "OriginalFolder": "data",
    "CdnImagePath": "images",
    "CdnFilePath": "files"
  },
  "Jwt": {
    "Key": "{APP_JWT_KEY}",
    "Issuer": "{APP_JWT_ISSUER}",
    "Audience": ""
  },
  "EndPointApi": {
    "Master": {
      "BaseUrl":  "{APP_BASE_ENDPOINT}/master-api",
      "AccessToken": ""
    },
    "Be": {
      "BaseUrl": "{APP_BASE_ENDPOINT}/master-api"
    }
  },
  "RabbitConfig": {
    "RabbitEnabled": true,
    "RabbitCachePubSubEnabled": true,
    "RabbitHostName": "{APP_RABBIT_HOST}",
    "RabbitVirtualHost": "{APP_RABBIT_VIRTUAL_HOST}",
    "RabbitUsername": "{APP_RABBIT_USERNAME}",
    "RabbitPassword": "{APP_RABBIT_PASSWORD}",
    "RabbitCacheReceiveEndpoint": "",
    "ConsumerEnabled": true,
    "PublisherEnabled": true
  },
  "AllowedHosts": "*"
}